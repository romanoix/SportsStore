@model IEnumerable<string>

@Html.ActionLink("Home","List","Product",null,
    new { @class = "card card-body bg-light " })

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
{
    controller = "Product",
    action = "List",
    category = link,
    page = 1
}, new
{
    @class = "card card-body bg-light "
})
}